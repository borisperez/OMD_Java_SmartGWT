<?xml version="1.0" encoding="UTF-8"?>
<Context antiJARLocking="true" path="/ETLPlus">
<Resource name="jdbc/etlplus" auth="Container" type="javax.sql.DataSource">
<ResourceParams name="jdbc/etlplus">
	<parameter>
		<name>factory</name>
		<value>org.apache.commons.dbcp.BasicDataSourceFactory</value>
	</parameter>
	<parameter>
		<name>driverClassName</name>
		<value>com.mysql.jdbc.Driver</value>
	</parameter>
	<parameter>
		<name>url</name>
		<value>jdbc:mysql://localhost:3306/omd</value>
	</parameter>
</ResourceParams>
</Resource>
</Context>